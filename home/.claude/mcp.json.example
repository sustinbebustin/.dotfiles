{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "x-api-key": "your-context7-api-key"
      }
    },
    "grep_app": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "opensrc": {
      "command": "npx",
      "args": ["-y", "opensrc-mcp"]
    },
    "overseer": {
      "command": "npx",
      "args": ["-y", "@dmmulroy/overseer", "mcp"]
    }
  }
}