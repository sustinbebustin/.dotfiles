[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[user]
	name = sustinbebustin
	email = 999@sustinbebustin.com
[alias]
	co = checkout
	br = branch
	st = status
	ci = commit
	unstage = reset HEAD --
	last = log -1 HEAD
[push]
	autoSetupRemote = true
	default = current
[pull]
	rebase = true
[rebase]
	autoStash = true
	autoSquash = true
[merge]
	conflictstyle = zdiff3
[diff]
	algorithm = histogram
[rerere]
	enabled = true
[fetch]
	prune = true
[init]
	defaultBranch = main
